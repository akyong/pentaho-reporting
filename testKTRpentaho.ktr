    <sql>select 
a.id, a.product_code, a.product_name, a.supplier_id, c.member_code, a.main_menu_id, d.mainmenu_name, a.sub_menu_id, e.submenu_name, 
a.sub_menucategory_id, f.submenu_category_name,
a.agent_price->'$.price[0].markup_price' as normal_price,
(a.agent_price->'$.price[0].markup_price'  - a.agent_price->'$.price[0].discount_member') as agent_price,
(a.agent_price->'$.price[0].markup_price'  - a.agent_price->'$.price[0].discount_member') as member_price,
a.agent_price->'$.price[0].agent_commision' as agent_commision,
b.id as variant_id, b.variant_descriptions, (SELECT  x.url FROM product_images x where x.product_details_id = a.id order by x.sequence asc, x.id desc limit 1) as main_image
from product_details a 
left join product_variation_details b on b.product_details_id = a.id 
inner join supplier c on c.id = a.supplier_id
left join main_menu d on d.id = a.main_menu_id
left join sub_menu e on e.id = a.sub_menu_id
left join sub_menu_category f on f.id = a.sub_menucategory_id group by a.id order by a.id asc</sql>
